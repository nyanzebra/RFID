<div>
	<%= form_tag(items_path, :method => "get", class: "navbar-form", id: "search-form") do %>
		<div class="input-append">
			<%= text_field_tag :search, params[:search], class: "span2", placeholder: "search" %>
			<button class="button" type="submit"><i class="icon-search">Search</i></button>
		</div>
	<% end %>
</div>